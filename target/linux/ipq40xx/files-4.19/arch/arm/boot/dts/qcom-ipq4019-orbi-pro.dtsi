#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	chosen {
		bootargs = "root=/dev/mmcblk0p20 rootfstype=squashfs,ext4 rootwait clk_ignore_unused";
	};

	aliases {
		led-boot = &white;
		led-failsafe = &red;
		led-running = &green;
		led-upgrade = &blue;
		ethernet0 = &gmac;
	};

	soc {
		rng@22000 {
			status = "okay";
		};

		counter@4a1000 {
			compatible = "qcom,qca-gcnt";
			reg = <0x4a1000 0x4>;
		};

		vqmmc: regulator@1948000 {
			compatible = "qcom,ipq40xx-vqmmc-regulator";
			reg = <0x01948000 0x4>;
			regulator-name = "vqmmc";
			regulator-min-microvolt = <1500000>;
			regulator-max-microvolt = <3000000>;
			regulator-always-on;
			status = "okay";
		};

		tcsr@1949000 {
			compatible = "qcom,tcsr";
			reg = <0x1949000 0x100>;
			qcom,wifi_glb_cfg = <TCSR_WIFI_GLB_CFG>;
		};

		tcsr@194b000 {
			compatible = "qcom,tcsr";
			reg = <0x194b000 0x100>;
			qcom,usb-hsphy-mode-select = <TCSR_USB_HSPHY_HOST_MODE>;
		};

		tcsr@1957000 {
			compatible = "qcom,tcsr";
			reg = <0x1957000 0x100>;
			qcom,wifi_noc_memtype_m0_m2 = <TCSR_WIFI_NOC_MEMTYPE_M0_M2>;
		};

		usb2@60f8800 {
			status = "okay";
		};

		usb3@8af8800 {
			status = "okay";
		};

		crypto@8e3a000 {
			status = "okay";
		};

		watchdog@b017000 {
			status = "okay";
		};
	};

	gpio-keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 45 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART >;
		};

		wps {
			label = "wps";
			gpios = <&tlmm 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	leds {
		compatible = "gpio-leds";

		white: white {
			label = "orbi:white:all";
			gpios = <&tlmm 60 GPIO_ACTIVE_HIGH>;
		};

		blue: blue {
			label = "orbi:blue:all";
			gpios = <&tlmm 57 GPIO_ACTIVE_HIGH>;
		};

		red: red {
			label = "orbi:red:all";
			gpios = <&tlmm 54 GPIO_ACTIVE_HIGH>;
		};

		green: green {
			label = "orbi:green:all";
			gpios = <&tlmm 53 GPIO_ACTIVE_HIGH>;
		};

		power-green {
			label = "orbi:power:green";
			gpios = <&tlmm 63 GPIO_ACTIVE_HIGH>;
			default-state = "on";
		};

		power-red {
			label = "orbi:power:red";
			gpios = <&tlmm 64 GPIO_ACTIVE_HIGH>;
			default-state = "off";
			panic-indicator;
		};
	};
};

&blsp1_uart1 {
	pinctrl-0 = <&serial_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&cryptobam {
	status = "okay";
};

&wifi0 {
	status = "okay";
	qcom,ath10k-calibration-variant = "Netgear-Orbi-Pro-SRK60";
};

&wifi1 {
	status = "okay";
	qcom,ath10k-calibration-variant = "Netgear-Orbi-Pro-SRK60";
};


&blsp_dma {
	status = "okay";
};

&tlmm {
	serial_pins: serial_pinmux {
		mux {
			pins = "gpio16", "gpio17";
			function = "blsp_uart0";
			bias-disable;
		};

		pulldowns {
			pins = "gpio16";
			bias-pull-down;
		};
	};

	i2c_0_pins: i2c_0_pinmux {
		pinmux {
			function = "blsp_i2c0";
			pins = "gpio58", "gpio59";
			bias-disable;
		};
		pinconf {
			pins = "gpio58", "gpio59";
			drive-strength = <16>;
			bias-disable;
		};
	};

	spi_0_pins: spi_0_pinmux {
		mux {
			pins = "gpio12", "gpio13", "gpio14", "gpio15";
			function = "blsp_spi0";
			bias-disable;
		};
	};

	sd_pins: sd_pins {
		pinmux {
			function = "sdio";
			pins = "gpio23", "gpio24", "gpio25", "gpio26",
				"gpio28", "gpio29", "gpio30", "gpio31";
			drive-strength = <10>;
		};

		pinmux_sd_clk {
			function = "sdio";
			pins = "gpio27";
			drive-strength = <16>;
		};

		pinmux_sd7 {
			function = "sdio";
			pins = "gpio32";
			drive-strength = <10>;
			bias-disable;
		};
	};

	mdio_pins: mdio_pinmux {
		mux_1 {
			pins = "gpio6";
			function = "mdio";
			bias-pull-up;
		};
		mux_2 {
			pins = "gpio7";
			function = "mdc";
			bias-pull-up;
		};
	};
};

&blsp1_spi1 { /* BLSP1 QUP1 */
	pinctrl-0 = <&spi_0_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&blsp1_i2c3 {
	pinctrl-0 = <&i2c_0_pins>;
	pinctrl-names = "default";

	status = "okay";
	
	tlc59208f@27 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "ti,tlc59108";
		reg = <0x27>;
		
		led0@0 {
			label = "orbi:led0:rgb";
			reg = <0x0>;
		};
		led1@1 {
			label = "orbi:led1:rgb";
			reg = <0x1>;
		};
		led2@2 {
			label = "orbi:led2:rgb";
			reg = <0x2>;
		};
		led3@3 {
			label = "orbi:led3:rgb";
			reg = <0x3>;
		};
		led4@4 {
			label = "orbi:led4:rgb";
			reg = <0x4>;
		};
		led5@5 {
			label = "orbi:led5:rgb";
			reg = <0x5>;
		};
		led6@6 {
			label = "orbi:led6:rgb";
			reg = <0x6>;
		};
		led7@7 {
			label = "orbi:led7:rgb";
			reg = <0x7>;
		};
	};
};

&usb3_ss_phy {
	status = "okay";
};

&usb3_hs_phy {
	status = "okay";
};

&usb2_hs_phy {
	status = "okay";
};

&pcie0 {
	status = "okay";
	perst-gpio = <&tlmm 38 GPIO_ACTIVE_LOW>;
	wake-gpio = <&tlmm 40 GPIO_ACTIVE_LOW>;

	bridge@0,0 {
		reg = <0x00000000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;
		ranges;

		wifi2: wifi@0,0 {
			compatible = "qcom,ath10k";
			status = "okay";
			reg = <0x00010000 0 0 0 0>;
			qcom,ath10k-calibration-variant = "Netgear-Orbi-Pro-SRK60";
		};
	};
};

&mdio {
	status = "okay";
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
};

&psgmii_phy {
	status = "okay";
};

&ess {
	status = "okay";
};

&gmac {
	status = "okay";
};

&essport1 {
	label = "wan";
};

&essport2 {
	label = "lan1";
};

&essport3 {
	label = "lan2";
};

&essport4 {
	label = "lan3";
};

/delete-node/ &essport5;

&sdhci {
	pinctrl-0 = <&sd_pins>;
	pinctrl-names = "default";
	cd-gpios = <&tlmm 22 0x1>;
	vqmmc-supply = <&vqmmc>;
	status = "okay";
};
