From dde0fb0f2ebe1699ad3852348a8189f541c1ac8a Mon Sep 17 00:00:00 2001
From: Robert Marko <robimarko@gmail.com>
Date: Tue, 8 May 2018 13:28:54 +0200
Subject: [PATCH] ipq40xx: Add SDHCI node to qcom-ipq4019.dtsi

Add SDHCI node that is needed to support SDHCI in IPQ4019 as the device supports is.
---
 qcom-ipq4019.dtsi | 10 ++++++++++
 1 file changed, 10 insertions(+)

diff --git a/arch/arm/boot/dts/qcom-ipq4019.dtsi b/arch/arm/boot/dts/qcom-ipq4019.dtsi
index 5c1b097..46fc9a6 100644
--- a/arch/arm/boot/dts/qcom-ipq4019.dtsi
+++ b/arch/arm/boot/dts/qcom-ipq4019.dtsi
@@ -667,6 +667,16 @@
 			};
 		};
 
+		sdhci: sdhci@7824000 {
+			compatible = "qcom,sdhci-msm-v4";
+			reg = <0x7824900 0x11c>, <0x7824000 0x800>;
+			interrupts = <0 123 0>, <0 138 0>;
+			bus-width = <8>;
+			clocks = <&gcc GCC_SDCC1_APPS_CLK>, <&gcc GCC_SDCC1_AHB_CLK>, <&xo_board>;
+			clock-names = "core", "iface", "xo";
+			status = "disabled";
+		};
+
 		usb3_ss_phy: ssphy@9a000 {
 			compatible = "qca,uni-ssphy";
 			reg = <0x9a000 0x800>;
